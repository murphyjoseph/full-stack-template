{
  "name": "@repo/api-schema",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./src/client.ts",
        "default": "./src/client.ts"
      }
    }
  },
  "scripts": {
    "generate": "openapi-typescript openapi.json -o src/schema.d.ts",
